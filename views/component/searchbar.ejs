              <div class="col-3 rowTwo">
                    <div  class="input-group mb-3 form-input item">
                          <span class="input-group-text form-input" >الصنف</span>
                          <select id="inputItemSearch" class="form-select" name="selectItem">
                              <option value="">تحديد صنف</option>
                            <%for(i of items){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                              <%}%>
                          </select>
                    </div>
               </div>
              <div class="col-3 ">
                  <div  class="input-group mb-3 form-input DebtorAccount">
                        <span class="input-group-text " style="margin-right: 6px;" >ح/المدين</span>
                        <select class="form-select" id="DebtorAccountSearch" name="DebtorAccount">
                            <option value="">تحديد حساب</option>

                          <optgroup label="الدليل المحاسبي" data-max-options="1">
                            <%for(i of guides){%>
                                <option value="<%=i._id%>"><%=i.name%></option>
                            <%}%>
                          </optgroup>
            
                          <optgroup label="الموردون" data-max-options="1">
                            <%for(i of suppliers){%>
                                <option value="<%=i._id%>"><%=i.name%></option>
                            <%}%>
                          </optgroup>
            
                            <optgroup label="العملاء" data-max-options="1">
                            <%for(i of clients){%>
                                <option value="<%=i._id%>"><%=i.name%></option>
                            <%}%>
                          </optgroup>
                          
                            <optgroup label="الموظفين" data-max-options="1">
                            <%for(i of employees){%>
                                <option value="<%=i._id%>"><%=i.name%></option>
                            <%}%>
                          </optgroup>
            
                            <optgroup label="التكلفة" data-max-options="1">
                            <%for(i of costs){%>
                                <option value="<%=i._id%>"><%=i.name%></option>
                            <%}%>
                          </optgroup>
                        </select>
                  </div>
              </div>
              <div class="col-3 ">
                  <div  class="input-group mb-3 form-input CreditorAccount">
                        <span class="input-group-text " style="margin-right: 6px;" >ح/الدائن</span>
                        <select  class="form-select" id="CreditorAccountSearch" name="CreditorAccount">
                            <option value="">تحديد حساب</option>

                          <optgroup label="الدليل المحاسبي" data-max-options="1">
                          <%for(i of guides){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                          <%}%>
                        </optgroup>
            
                        <optgroup label="الموردون" data-max-options="1">
                          <%for(i of suppliers){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                          <%}%>
                        </optgroup>
            
                          <optgroup label="العملاء" data-max-options="1">
                          <%for(i of clients){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                          <%}%>
                        </optgroup>
                        
                          <optgroup label="الموظفين" data-max-options="1">
                          <%for(i of employees){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                          <%}%>
                        </optgroup>
            
                          <optgroup label="التكلفة" data-max-options="1">
                          <%for(i of costs){%>
                              <option value="<%=i._id%>"><%=i.name%></option>
                          <%}%>
                        </optgroup>
                        </select>
                  </div>
              </div>
              <div  class="input-group mb-3 form-input   ">
              <div class="col-1 submit ">
                <button class="btn btn-success " style="margin-right: 1px;" type="submit">بحث</button>
              </div>
            </div>
            
