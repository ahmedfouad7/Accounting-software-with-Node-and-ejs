                      <input class="form-control me-2" value="<%=query.fromDate%>" name="fromDate" type="date" placeholder="بحث بالتاريخ" aria-label="Search">
                      <%}else{%>
                      <input class="form-control me-2" name="fromDate" type="date" placeholder="بحث بالتاريخ" aria-label="Search">
                        <%}%>
                  </div>
              </div>
              <div class="col-3 ">
                  <div  class="input-group mb-3 form-input dateTo">
                      <span class="input-group-text form-input">الي</span>
                      <%if(query.toDate){%>
